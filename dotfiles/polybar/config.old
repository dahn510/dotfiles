include-file = ~/.config/polybar/modules/modules.ini

[bar/mybar]
wm-restack = true
modules-right = date
modules-left = battery

[module/date]
type = internal/date
date = %Y-%m-%d%
time = %H:%M
label = %time% %date%

[module/battery]
type = internal/battery

battery = BAT0
adapter = AC

label-charging = Charging %percentage%%
label-discharging = Discharging %percentage%%
